
<h1>Listing Users</h1>
<%= link_to 'New Admin', create_admin_path %>
<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th colspan="3"></th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Show', user_list_path(user) %></td>
      <td><%= link_to 'Edit', edit_user_password_path(user) %></td>
      <td><%= link_to 'Destroy', user_list_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>